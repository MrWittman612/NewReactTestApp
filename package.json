{
  "name": "newreacttestapp",
  "version": "1.0.0",
  "description": "This Is a play ground Full Stack App for me to test package func Built with express react",
  "main": "server/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier \"./client/src/**/*.{js.html}\" --write",
    "lint": "eslint \"./server/**/*.{js.jsx}\" --quiet",
    "client-install": "npm install --prefix client",
    "start": "node server/server.js",
    "server": "nodemon server/server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "npm install && cd client && npm install",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    }
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrWittman612/NewReactTestApp.git"
  },
  "author": "Chad Wittman",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.2",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0"
  },
  "devDependencies": {
    "eslint": "^6.1.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-standard": "^4.0.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2"
  },
  "root": true
}
