{
  "name": "newreacttestapp",
  "version": "1.0.0",
  "description": "This Is a play ground Full Stack App for me to test package func Built with express react",
  "main": "server/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier \"client/src/**/*.{js.html}\" --write",
    "lint": "eslint \"client/src/**/*.{js.jsx}\" --quiet",
    "client-install": "npm install --prefix client",
    "start": "node server/server.js",
    "server": "nodemon server/server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "npm install && cd client && npm install",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    }
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrWittman612/NewReactTestApp.git"
  },
  "author": "Chad Wittman",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.1",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.2",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2"
  },
  "root": true
}
